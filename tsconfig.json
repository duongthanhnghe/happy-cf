{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    // "types": [
    //   "@types/node",
    //   "nuxt",
    //   "@nuxt/schema",
    // ],
    "strict": true,
    "allowJs": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "ignoreDeprecations": "6.0",
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "@server/*": ["server/*"],
    }
  },
  "include": [
    ".nuxt",
    "env.d.ts",
    "global.d.ts",
    "**/*.ts",
    "**/*.js",
    "**/*.vue","plugins/vuetify.ts"
  ],
  "exclude": [
    "node_modules",
    ".output",
    "dist"
  ]
}
